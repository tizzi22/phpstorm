<templateSet group="Doc">
  <template name="aa" value="@author Rusavskiy Vitaliy &lt;rusavskiy.v@bintime.com&gt;" description="author" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CoffeeScript" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="PHP Comment" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="dd" value="@date $DATE$" description="Date" toReformat="false" toShortenFQNames="true">
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="cc" value="@author Rusavskiy Vitaliy &lt;rusavskiy.v@bintime.com&gt;&#10;* @date $DATE$" description="author &amp;&amp; Date" toReformat="false" toShortenFQNames="true">
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="hh" value="/**&#10; * @link http://www.bintime.com&#10; * @copyright $YEAR$ (c) Bintime&#10; * @package $NAMESPACE$&#10; * @author Rusavskiy Vitaliy &lt;rusavskiy.v@bintime.com&gt;&#10; */&#10; " description="header file" toReformat="true" toShortenFQNames="true">
    <variable name="YEAR" expression="date(&quot;Y&quot;)" defaultValue="" alwaysStopAt="false" />
    <variable name="NAMESPACE" expression="${NAMESPACE}" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>